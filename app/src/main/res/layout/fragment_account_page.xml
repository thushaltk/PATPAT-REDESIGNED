<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".AccountPageFragment"
    android:background="@color/home_color">

    <View
        android:id="@+id/view_acntPage"
        android:layout_width="256dp"
        android:layout_height="634dp"
        android:layout_marginBottom="60dp"
        android:background="@drawable/rounded_curve"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />


    <TextView
        android:id="@+id/textView_acntPage"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="120dp"
        android:fontFamily="@font/rubik_medium"
        android:text="Mrs. Kalpani Perera"
        android:textColor="@color/white"
        android:textSize="25dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.583"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView33"
        android:layout_width="93dp"
        android:layout_height="93dp"
        android:src="@drawable/profile_dp"
        app:layout_constraintBottom_toTopOf="@+id/textView_acntPage"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.534"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.521" />

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="524dp"
        android:background="@drawable/white_rounded_rectangle"
        android:elevation="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />


    <TextView
        android:id="@+id/textView_welcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:elevation="2dp"
        android:text="@string/welcome"
        android:textColor="@color/black"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <ScrollView
        android:layout_width="415dp"
        android:layout_height="383dp"
        android:layout_marginTop="32dp"
        android:elevation="3dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView_welcome">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="112dp"
                android:layout_height="104dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_my_ad"
                app:layout_constraintBottom_toBottomOf="@+id/btn_icon_my_ad"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_my_ad"
                app:layout_constraintHorizontal_bias="0.47"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_my_ad"
                app:layout_constraintTop_toTopOf="@+id/btn_icon_my_ad"
                app:layout_constraintVertical_bias="0.545" />

            <Button
                android:id="@+id/btn_icon_my_ad"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:background="@drawable/button_background"
                app:layout_constraintEnd_toStartOf="@+id/imageView13"
                app:layout_constraintHorizontal_bias="0.747"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/imageView13"
                android:layout_width="112dp"
                android:layout_height="104dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_orderinq"
                app:layout_constraintBottom_toBottomOf="@+id/btn_icon_query_inquiry"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_query_inquiry"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_query_inquiry"
                app:layout_constraintTop_toTopOf="@+id/btn_icon_query_inquiry" />

            <Button
                android:id="@+id/btn_icon_query_inquiry"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:layout_marginEnd="76dp"
                android:background="@drawable/button_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />


            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="112dp"
                android:layout_height="104dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_fav"
                app:layout_constraintBottom_toBottomOf="@+id/btn_icon_favorites"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_favorites"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_favorites"
                app:layout_constraintTop_toTopOf="@+id/btn_icon_favorites" />


            <Button
                android:id="@+id/btn_icon_favorites"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:layout_marginStart="76dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/button_background"
                app:layout_constraintEnd_toStartOf="@+id/btn_icon_gotohome"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView12" />

            <ImageView
                android:id="@+id/imageView16"
                android:layout_width="112dp"
                android:layout_height="104dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_gotohome"
                app:layout_constraintBottom_toBottomOf="@+id/btn_icon_gotohome"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_gotohome"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_gotohome"
                app:layout_constraintTop_toTopOf="@+id/btn_icon_gotohome" />


            <Button
                android:id="@+id/btn_icon_gotohome"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="76dp"
                android:background="@drawable/button_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView13" />

            <ImageView
                android:id="@+id/imageView17"
                android:layout_width="104dp"
                android:layout_height="94dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_financialrequest"
                app:layout_constraintBottom_toBottomOf="@+id/btn_icon_financialrequest"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_financialrequest"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_financialrequest"
                app:layout_constraintTop_toTopOf="@+id/btn_icon_financialrequest" />

            <Button
                android:id="@+id/btn_icon_financialrequest"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:layout_marginStart="76dp"
                android:layout_marginTop="20dp"
                android:background="@drawable/button_background"
                app:layout_constraintEnd_toStartOf="@+id/btn_icon_editmyprofile"
                app:layout_constraintHorizontal_bias="0.0"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_icon_favorites" />

            <ImageView
                android:id="@+id/imageView18"
                android:layout_width="104dp"
                android:layout_height="94dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_editmyprofile"
                app:layout_constraintBottom_toBottomOf="@+id/btn_icon_editmyprofile"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_editmyprofile"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_editmyprofile"
                app:layout_constraintTop_toTopOf="@+id/btn_icon_editmyprofile" />

            <Button
                android:id="@+id/btn_icon_editmyprofile"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="76dp"
                android:background="@drawable/button_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_icon_gotohome" />


            <ImageView
                android:id="@+id/imageView19"
                android:layout_width="113dp"
                android:layout_height="102dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_logout"
                app:layout_constraintBottom_toBottomOf="@+id/btn_icon_logout"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_logout"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_logout"
                app:layout_constraintTop_toTopOf="@+id/btn_icon_logout" />


            <Button
                android:id="@+id/btn_icon_logout"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:layout_marginStart="76dp"
                android:layout_marginTop="28dp"
                android:background="@drawable/button_background"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/imageView17" />


            <ImageView
                android:id="@+id/imageView20"
                android:layout_width="113dp"
                android:layout_height="102dp"
                android:background="@color/transparent"
                android:elevation="5dp"
                android:src="@drawable/icon_dltacnt"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="@+id/btn_icon_deleteaccount"
                app:layout_constraintHorizontal_bias="0.428"
                app:layout_constraintStart_toStartOf="@+id/btn_icon_deleteaccount" />

            <Button
                android:id="@+id/btn_icon_deleteaccount"
                android:layout_width="120dp"
                android:layout_height="119dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="76dp"
                android:background="@drawable/button_background"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/btn_icon_editmyprofile" />


        </androidx.constraintlayout.widget.ConstraintLayout>


    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>