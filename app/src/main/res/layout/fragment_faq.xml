        app:layout_constraintEnd_toEndOf="@+id/edit_faq_02"
        app:layout_constraintHorizontal_bias="0.96"
        app:layout_constraintStart_toStartOf="@+id/edit_faq_02"
        app:layout_constraintTop_toTopOf="@+id/edit_faq_02"
        app:layout_constraintVertical_bias="0.529" />

