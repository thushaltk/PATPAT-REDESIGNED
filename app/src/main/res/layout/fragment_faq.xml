        app:layout_constraintTop_toTopOf="@+id/edit_faq_03" />

    <EditText
        android:id="@+id/edit_faq_04"
        android:layout_width="355dp"
        android:layout_height="58dp"
        android:layout_marginTop="272dp"
        android:background="@drawable/edit_text_transparent"
        android:elevation="2dp"
        android:focusable="false"
        android:hint="How do I edit and delete
         advertisements?"
        android:inputType="none"
        android:paddingLeft="10dp"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view2" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:elevation="2dp"
        android:src="@drawable/ic_baseline_arrow_drop_down_circle_24"
        app:layout_constraintBottom_toBottomOf="@+id/edit_faq_04"
        app:layout_constraintEnd_toEndOf="@+id/edit_faq_04"
        app:layout_constraintHorizontal_bias="0.96"
        app:layout_constraintStart_toStartOf="@+id/edit_faq_04"
        app:layout_constraintTop_toTopOf="@+id/edit_faq_04"
        app:layout_constraintVertical_bias="0.47" />

